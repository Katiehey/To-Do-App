{
  "name": "taskmaster-pro-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "ANALYZE=true vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "build:clean": "npm run clean && npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.{js,jsx}",
    "test:integration": "vitest run --reporter=verbose \"src/**/__tests__/*.integration.test.*\"",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:ci": "vitest run --coverage --reporter=json --reporter=html"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.5",
    "react-datepicker": "^9.1.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.3.6",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^0.12.0",
    "vitest": "^4.0.18"
  }
}
